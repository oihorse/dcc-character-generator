<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HorizontalPanel>
		<g:VerticalPanel>
		<g:HorizontalPanel>
				<g:ListBox ui:field="armorList"></g:ListBox>
				<g:Button ui:field="addArmor">Add</g:Button>
				<g:Button ui:field="closeArmorButton">x</g:Button>
			</g:HorizontalPanel> 
			<g:HorizontalPanel ui:field="armorPanel">
				
			</g:HorizontalPanel>
		</g:VerticalPanel>
		<g:VerticalPanel>
				<g:Button ui:field="openArmorSelection">+</g:Button>
		</g:VerticalPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 